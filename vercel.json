{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/performance.ts": {
      "maxDuration": 30
    },
    "api/generate.ts": {
      "maxDuration": 30
    },
    "api/**/*.ts": {
      "excludeFiles": "**/lib/**"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api|assets|favicon.ico|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
